{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "FilePath": "logs/uchat-{Date}.log"
  },
  "SignalR": {
    "TimeoutSeconds": 30,
    "RetryDelays": [2, 2, 5, 10, 15, 30]
  },
  "Session": {
    "Directory": "~/.uchat"
  }
}
